+static int ssh_set_nonblock(int fd) {
+    int arg;
+    if ((arg = fcntl(fd, F_GETFL, NULL)) < 0) {
+        fprintf(stderr, "Error fcntl(..., F_GETFL) (%s)\n", strerror(errno));
+        return -1;
+    }
+    arg |= O_NONBLOCK;
+    if (fcntl(fd, F_SETFL, arg) < 0) {
+        fprintf(stderr, "Error fcntl(..., F_GETFL) (%s)\n", strerror(errno));
+        return -1;
+    }
+    return 0;
+}
+    
+static int ssh_unset_nonblock(int fd) {
+    int arg;
+    
+    if ((arg = fcntl(fd, F_GETFL, NULL)) < 0) {
+        fprintf(stderr, "Error fcntl(..., F_GETFL) (%s)\n", strerror(errno));
+        return -1;
+    }
+    arg &= (~O_NONBLOCK);
+    if (fcntl(fd, F_SETFL, arg) < 0) {
+        fprintf(stderr, "Error fcntl(..., F_GETFL) (%s)\n", strerror(errno));
+        return -1;
+    }
+    return 0;
+}
