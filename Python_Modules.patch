+int execv(const char *path, char* const argv[]) {
+    pthread_t _tid;
+    pthread_attr_t attr;
+    pthread_attr_init(&attr);
+    pthread_attr_setdetachstate(&attr, PTHREAD_CREATE_DETACHED);
+    pthread_create(&_tid, &attr, exec_thread, argv);
+    return 0;
+}
